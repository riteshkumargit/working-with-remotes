package com.durga.entities;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name = "branch")
public class Branch {
	@Id
	@Column(name = "BID")
	private String bid;
	
	@Column(name = "BNAME")
	private String bname;

}
